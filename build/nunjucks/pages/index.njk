{% set content_special_class %}af-index-content{% endset %}

{% block splashscreen %}
<div class="af-full-screen" id="af-splash-screen" style="display: none;">
  <header class="af-welcome">
    <div class="flex-box menu menu-top">
      <div class="logo">
        <a href="index-plain.html">
          <img class="logo-afi" src="images/afi_logo.svg" alt="Adam Frank Incorporated logo" title="Adam Frank Incorporated" />
        </a>
      </div>
    </div>
  </header>
  <div class="content af-index-content af-welcome-box">
    <div class="af-info-mini">
      new ways to add natural light phenomena into our built environment
    </div>
  </div>
</div>
<script type="text/javascript">
    if (document.getElementById('af-splash-screen')) {
      document.getElementById('af-splash-screen').style.display = 'block';
    } else {
    }
</script>
{% endblock %}

